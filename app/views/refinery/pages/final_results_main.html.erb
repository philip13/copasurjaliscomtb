<div class="resultados-2016">
  <div class="container">
    <div class="col-sm-12 text-center">
      <%= raw @page.content_for(:content) %>
    </div>
    <div class="categories">
      <% @page.children.each do |category| %>
        <a href="<%= "#{request.path}/#{category.slug}"%>">
          <div class="col-md-4">
            <h3 class="text-center"><%= category.slug.titleize%></h3>
          </div>
        </a>
      <%end%>
    </div>
  </div>
</div>