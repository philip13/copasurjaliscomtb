<%= form_for [refinery, :race_results_admin, @race_result] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @race_result,
              :include_object_name => true %>

  <div class='field'>
    <%= f.label :rider_id -%>
    <%= f.number_field :rider_id -%>
  </div>

  <div class='field'>
    <%= f.label :event_id -%>
    <%= f.number_field :event_id -%>
  </div>

  <div class='field'>
    <%= f.label :category_id -%>
    <%= f.number_field :category_id -%>
  </div>

  <div class='field'>
    <%= f.label :rider_number -%>
    <%= f.number_field :rider_number -%>
  </div>

  <div class='field'>
    <%= f.label :number_place -%>
    <%= f.number_field :number_place -%>
  </div>

  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.race_results.admin.race_results.race_result'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete') -%>
<% end -%>
